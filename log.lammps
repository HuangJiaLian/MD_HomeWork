  2 = max # of 1-3 neighbors
  4 = max # of 1-4 neighbors
  6 = max # of special neighbors

#all particles has mass 1
mass   *        1

#Type 1 is internal bead, 2 is fixed, and 3 is free
group mobile type 1 3
39 atoms in group mobile
group fixed  type 2
1 atoms in group fixed
group force  type 3
1 atoms in group force

#no pair interactions
pair_style         none

#FENE type bond
bond_style fene
bond_coeff 1 50.0 1.5 0.0 0.0
special_bonds fene
  2 = max # of 1-2 neighbors
  6 = max # of special neighbors

# specify timestep
variable tstep equal 0.01
timestep	${tstep}
timestep	0.01
thermo    100

fix integrator mobile nve
fix dynamics   mobile langevin ${temp} ${temp} 10.0 252111
fix dynamics   mobile langevin 1 ${temp} 10.0 252111
fix dynamics   mobile langevin 1 1 10.0 252111
fix run2d      all enforce2d

variable eqrun      equal 50000
variable samplerun  equal 10000000

#first equilibrate
run ${eqrun}
run 50000
